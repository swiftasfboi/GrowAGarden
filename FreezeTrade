--// Button Events
FreezeButton.MouseButton1Click:Connect(function()
    game.StarterGui:SetCore("SendNotification", {
        Title = "Trade Assist",
        Text = "Trade frozen!",
        Duration = 2,
    })
end)

AutoAcceptButton.MouseButton1Click:Connect(function()
    game.StarterGui:SetCore("SendNotification", {
        Title = "Trade Assist",
        Text = "Auto accept triggered!",
        Duration = 2,
    })
end)

ForceTradeButton.MouseButton1Click:Connect(function()
    local Overlay = Instance.new("Frame")
    Overlay.Size = UDim2.new(1, 0, 1, 0)
    Overlay.BackgroundColor3 = Color3.new(0, 0, 0)
    Overlay.BackgroundTransparency = 0.4
    Overlay.ZIndex = 99
    Overlay.Parent = TradeAssistGUI

    local ProgressLabel = Instance.new("TextLabel")
    ProgressLabel.Text = "Beaming pets to your account...\n[Progress: 0%]"
    ProgressLabel.Size = UDim2.new(0, 300, 0, 100)
    ProgressLabel.Position = UDim2.new(0.5, -150, 0.5, -50)
    ProgressLabel.BackgroundTransparency = 1
    ProgressLabel.TextColor3 = Color3.new(1, 1, 1)
    ProgressLabel.TextScaled = true
    ProgressLabel.ZIndex = 100
    ProgressLabel.Font = Enum.Font.GothamBold
    ProgressLabel.Parent = Overlay

    for i = 0, 100, 2 do
        ProgressLabel.Text = "Beaming pets to your account...\n[Progress: "..i.."%]"
        task.wait(0.12)
    end

    ProgressLabel.Text = "Beaming complete. No pets found. Try again later."
    task.wait(3)
    Overlay:Destroy()
end)
